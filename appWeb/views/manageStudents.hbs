<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="Content-Script-Type" content="text/javascript" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="Universidad de murcia">
    <title>Manage Students</title>


    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet">

    <link href="/stylesheets/main.css" rel="stylesheet">

    <link rel="stylesheet" href="/stylesheets/buttonBlank.css">
    <link rel="stylesheet" href="/stylesheets/switch.css">
    <link rel="stylesheet" href="/stylesheets/card.css">
    <link rel="stylesheet" href="/stylesheets/buttonSend.css">
</head>

<body>

     <div class="p-5 mb-4 bg-light rounded-3">
        <div class="container-fluid py-5">
            <h1 class="display-5 fw-bold g-col-12">Available Units</h1>
            <div id="temario" class="row">
                <div class="card col-sm-4">
                    <h5 class="fw-bold">UK General knowledge</h5>
                    <label class="switch">
                        <input type="checkbox" id="UKgk">
                        <span class="slider"></span>
                    </label>
                </div>
                <div class="card col-sm-4">
                    <h5 class="fw-bold">UK Geography</h5>
                    <label class="switch">
                        <input type="checkbox" id="UKgeo">
                        <span class="slider"></span>
                    </label>
                </div>
                <div class="card col-sm-4">
                    <h5 class="fw-bold">UK History</h5>
                    <label class="switch">
                        <input type="checkbox" id="UKhist">
                        <span class="slider"></span>
                    </label>
                </div>
                <div class="card col-sm-4">
                    <h5 class="fw-bold">UK Society</h5>
                    <label class="switch">
                        <input type="checkbox" id="UKsoc">
                        <span class="slider"></span>
                    </label>
                </div>
                <div class="card col-sm-4">
                    <h5 class="fw-bold">UK Mix</h5>
                    <label class="switch">
                        <input type="checkbox" id="UKmix">
                        <span class="slider"></span>
                    </label>
                </div>
                <div class="card col-sm-4">
                    <h5 class="fw-bold">USA General knowledge</h5>
                    <label class="switch">
                        <input type="checkbox" id="USAgk">
                        <span class="slider"></span>
                    </label>
                </div>
                <div class="card col-sm-4">
                    <h5 class="fw-bold">USA Geography</h5>
                    <label class="switch">
                        <input type="checkbox" id="USAgeo">
                        <span class="slider"></span>
                    </label>
                </div>
                <div class="card col-sm-4">
                    <h5 class="fw-bold">USA History</h5>
                    <label class="switch">
                        <input type="checkbox" id="USAhist">
                        <span class="slider"></span>
                    </label>
                </div>
                <div class="card col-sm-4">
                    <h5 class="fw-bold">USA Society</h5>
                    <label class="switch">
                        <input type="checkbox" id="USAsoc">
                        <span class="slider"></span>
                    </label>
                </div>
                <div class="card col-sm-4">
                    <h5 class="fw-bold">USA Mix</h5>
                    <label class="switch">
                        <input type="checkbox" id="USAmix">
                        <span class="slider"></span>
                    </label>
                </div>
            </div>
        </div>
        <button class="buttonSend" id="buttonTemas" onclick="updateTemas()">
            <div class="svg-wrapper-1">
                <div class="svg-wrapper">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="24" height="24">
                        <path fill="none" d="M0 0h24v24H0z"></path>
                        <path fill="currentColor"
                            d="M1.946 9.315c-.522-.174-.527-.455.01-.634l19.087-6.362c.529-.176.832.12.684.638l-5.454 19.086c-.15.529-.455.547-.679.045L12 14l6-8-8 6-8.054-2.685z">
                        </path>
                    </svg>
                </div>
            </div>
            <span>Send</span>
        </button>
    </div>

    <div class="p-5 mb-4 bg-light rounded-3">
        <div class="container-fluid py-5">
            <h1 class="display-5 fw-bold">Manage Students</h1>
            <table class="table table-striped table-hover" id="tablaAlumnos">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Name</th>
                        <th scope="col">Mail</th>
                        <th scope="col">Options</th>
                    </tr>
                </thead>
                <tbody id="tBody">
                </tbody>
            </table>
        </div>
    </div>


    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
        integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.10.2/dist/umd/popper.min.js"
        integrity="sha384-7+zCNj/IqJ95wo16oMtfsKbZ9ccEh31eOz1HGyDuCQ6wgnyJNSYdrPa03rtR1zdB"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.min.js"
        integrity="sha384-QJHtvGhmr9XOIpI6YVutG+2QOK9T+ZnN4kzFN1RtK3zEFEIsxhlmWl5/YESvpZ13"
        crossorigin="anonymous"></script>
    <script src="/javascripts/manageStudents.js"></script>
</body>

</html>