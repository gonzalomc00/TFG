<body>

  <div class="b-example-divider"></div>
  <main>
    <div class="container marketing">
      <div class="row">
        <div class="col-lg-4">
          <h2>Ranking Gold Medals</h2>
          <table class="table">
            <thead class="table-dark">
              <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Points</th>
              </tr>
            </thead>
            <tbody id="goldMedal">
              {{#each medallas}}
              {{#if mail}}
              <tr style="background-color: yellowgreen;">
              {{else}}
              <tr>
              {{/if}}
                <th scope="row">{{this.ind}}</th>
                <td>{{this.name}}</td>
                <td>{{this.vitrina.medallaOro}}</td>
              </tr>
              {{/each}}
            </tbody>
          </table>
        </div><!-- /.col-lg-4 -->
        <div class="col-lg-4">
          <h2>Ranking Infinite Mode</h2>
          <table class="table">
            <thead class="table-dark">
              <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Points</th>
              </tr>
            </thead>
            <tbody id="infinite">
                {{#each records}}
              {{#if mail}}
              <tr style="background-color: yellowgreen;">
              {{else}}
              <tr>
              {{/if}}
                <th scope="row">{{this.ind}}</th>
                <td>{{this.name}}</td>
                <td>{{this.vitrina.recordInfinito}}</td>
              </tr>
              {{/each}}
            </tbody>
          </table>
        </div><!-- /.col-lg-4 -->
        <div class="col-lg-4">
          <h2>Ranking Gold Trophies</h2>
          <table class="table">
            <thead class="table-dark">
              <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Points</th>
              </tr>
            </thead>
            <tbody id="trophies">
              {{#each trofeos}}
              {{#if mail}}
              <tr style="background-color: yellowgreen;">
              {{else}}
              <tr>
              {{/if}}
                <th scope="row">{{this.ind}}</th>
                <td>{{this.name}}</td>
                <td>{{this.vitrina.trofeo}}</td>
              </tr>
              {{/each}}
            </tbody>
          </table>
        </div><!-- /.col-lg-4 -->
      </div><!-- /.row -->
    </div>
  </main>
