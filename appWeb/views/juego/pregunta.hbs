<body>
    <main id="main">
        <div id="indice" style="float: left;margin: 20px;">
        </div>

        <div id="clock">
            <span id="seconds" class="spanClock">45</span>
        </div>

        <div class="card" style="float: left; margin:50px;">
            <p id="pregunta" style="color: black;padding:10px;font-size: large;"></p>
        </div>

        <div>
            <h4 id="comodines" style="display: flex;justify-content: center;padding: 20px;"></h4>
        </div>

        <div id="escenario" style="width: 100%;">
            <img width="100%" style="padding-left: 10%;padding-right: 10%; max-height: 30vw;" id="img">
            </img>
        </div>

        <div id="response" style="align-items: center;justify-content: center;flex-wrap: wrap;flex-direction: row;align-content: center;">

        </div>
    </main>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
        integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
        crossorigin="anonymous"></script>
